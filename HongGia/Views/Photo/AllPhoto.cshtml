@using HongGia.Helpers

@model HongGia.Models.AllPhotoViewModel

@{
    ViewBag.Title = "All Photo";
}


<div class="row" style="margin-top:20px; margin-bottom: 20px;">
    <div class="col-md-12">
        
        @foreach (var category in @Model.Categories)
        {
            @Html.ColllapsiblePhotoPanel(@Model.AllPhoto.Where(x=> x.Categories.Contains(category)), category)
        }

    </div>
</div>
